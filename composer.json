{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "phpdocumentor/reflection-docblock": "^5.3",
        "spatie/emoji": "^4.0",
        "symfony/console": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/error-handler": "^6.2",
        "symfony/filesystem": "^6.2",
        "symfony/finder": "^6.2",
        "symfony/flex": "^v1.19",
        "symfony/framework-bundle": "^v6.2",
        "symfony/http-client": "^6.2",
        "symfony/messenger": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/property-info": "^6.2",
        "symfony/serializer": "^6.2",
        "symfony/validator": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-symfony": "^1.2",
        "symfony/debug-bundle": "^6.2",
        "symfony/var-dumper": "^6.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.2"
        }
    }
}
